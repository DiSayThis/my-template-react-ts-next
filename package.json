{
	"name": "my-template-react-ts-next",
	"version": "0.1.0",
	"private": true,
	"keywords": [
		"template",
		"typescript",
		"react",
		"next",
		"eslint",
		"linters"
	],
	"scripts": {
		"dev": "next dev",
		"debug": "cross-env NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"lint:format": "prettier -c .",
		"lint:format-fix": "prettier --write .",
		"lint:css": "stylelint \"./src/**/*.css\"",
		"lint:css-fix": "npm run lint:css -- --fix",
		"lint:all": "npm run lint:format && npm run lint && npm run lint:css",
		"lint:all-fix": "npm run lint:fix && npm run lint:format-fix && npm run lint:css-fix",
		"lint:staged": "npx lint-staged",
		"husky:addLint": "npx husky add .husky/pre-commit 'npx tsc --pretty --noEmit && npx lint-staged'",
		"prepare": "husky install && npm run husky:addLint"
	},
	"dependencies": {
		"@next/font": "13.1.6",
		"next": "13.1.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "4.9.5"
	},
	"devDependencies": {
		"@disaythis/gen-rtsc": "^1.0.4",
		"@svgr/webpack": "^7.0.0",
		"@types/classnames": "^2.3.1",
		"@types/node": "^18.13.0",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"classnames": "^2.3.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-jest-enzyme": "^7.1.2",
		"eslint-config-next": "^13.1.6",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.1",
		"prettier": "^2.8.4",
		"sass": "^1.61.0",
		"stylelint": "^15.0.0",
		"stylelint-config-clean-order": "^2.3.1",
		"stylelint-config-standard": "^30.0.1",
		"stylelint-order": "^6.0.2"
	}
}
